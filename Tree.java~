  public boolean isEmpty()
  {
    return node==null;
  }

   public void setRoot(int data)
  {
    root = new Node4(data);
  }
  public Node4 getRoot()
  {
    return root;
  }
  public void insert(Node4 node, int data)
  {
    
  }
  public void print(Node4 r)
  {
    if (r!=null)
    {
      print(r.left);
      System.out.println(r.data+ ", " );
      print(r.right);
    }
